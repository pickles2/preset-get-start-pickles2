{
    "name": "更新履歴 - Change Log",
    "interface": {
        "fields": {
            "header": {
                "fieldType": "input",
                "type": "multitext",
                "label": "リスト見出し",
                "rows": 1,
                "default": {
                    "editor": "markdown",
                    "src": "## 更新履歴"
                }
            },
            "loop": {
                "fieldType": "loop",
                "label": "履歴情報"
            }
        },
        "subModule": {
            "loop": {
                "fields": {
                    "date": {
                        "fieldType": "input",
                        "type": "text",
                        "label": "日付",
                        "rows": 1,
                        "description": "例: 2019年1月30日"
                    },
                    "contents": {
                        "fieldType": "input",
                        "type": "multitext",
                        "label": "内容"
                    }
                }
            }
        }
    }
}
