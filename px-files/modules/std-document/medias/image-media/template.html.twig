<figure class="px2-embedded-media{% if _ENV.vars.framed %} px2-embedded-media--framed{% endif %}">
  <div class="px2-embedded-media__image-media"><img src="{{ image_src }}" alt="{{ alt_text }}" /></div>
  {% if _ENV.vars.caption %}
  <figcaption class="px2-embedded-media__caption">{{ caption }}</figcaption>
  {% endif %}
</figure>
