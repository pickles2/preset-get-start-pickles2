{
    "name": "pickles2/preset-get-start-pickles2",
    "description": "Pickles 2 は、静的で大きなウェブサイトを効率よく構築できる オープンソースのHTML生成ツールです。",
    "keywords": ["pickles2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomoya Koyanagi",
            "email": "tomk79@gmail.com"
        }
    ],
    "config": {
        "process-timeout": 0
    },
	"scripts": {
		"start": [
			"@php ./src_px2/.px_execute.php \"/?PX=serve\""
		],
		"start-pub": [
			"@php ./src_px2/.px_execute.php \"/?PX=serve.pub\""
		]
	},
    "repositories": [
    ],
    "require": {
        "php": ">=7.3.0" ,
        "pickles2/px-fw-2.x": "^2.0",
        "pickles2/px2-dec": "^2.0",
        "pickles2/px2-multitheme": "^2.0",
        "pickles2/px2-path-resolver": "^2.0",
        "pickles2/px2-px2dthelper": "^2.0",
        "pickles2/px2-remove-attr": "^2.0",
        "pickles2/px2-sitemapexcel": "^2.0",
        "pickles2/px2-publish-ex": "^2.0",
        "pickles2/px2style": "^3.1",
        "tomk79/px2-scss": "^0.1.0",
        "tomk79/px2-serve": "~0.1.3",
		"vlucas/phpdotenv": "~5.4.0"
    },
    "require-dev": {
    },
    "autoload": {
        "files": [
        ]
    },
    "extra": {
        "px2package": {
            "name": "Get start \"Pickles 2\" !",
            "type": "project",
            "path": "src_px2/.px_execute.php",
            "path_homedir": "px-files/"
        }
    }
}
